{
    "remotes": [
        {
            "name": "one",
            "address": "foo"
        },
        {
            "name": "two",
            "address": "bar"
        }
    ],
    "modules": [
        {
            "name": "my-module",
            "executable_path": "${packages.modules.my-module}/exec.sh",
            "log_level": "info"
        }
    ],
    "packages": [
        {
            "name": "my-module",
            "version": "latest",
            "package": "org/my-module",
            "type": "module"
        },
        {
            "name": "my-ml-package",
            "version": "3",
            "package": "org/my-ml-package",
            "type": "ml_model"
        }, 
        {
            "name": "cool-package", 
            "version": "2", 
            "package": "org/cool-package"
        }
    ],
    "services": [
        {
            "attributes": {
              "label_path": "${packages.ml_models.my-ml-package}/effdetlabels.txt",
              "num_threads": 1,
              "model_path": "${packages.ml_models.my-ml-package}/effdet0.tflite"
            },
            "name": "face-detection",
            "type": "mlmodel",
            "model": "tflite_cpu"
          }, 
          {
            "attributes": {
              "label_path": "${packages.cool-package}/coollabels.txt",
              "num_threads": 1,
              "model_path": "${packages.cool-package}/cool.tflite"
            },
            "name": "face-detection",
            "type": "mlmodel",
            "model": "tflite_cpu"
          }
    ],
    "components": [
        {
            "name": "pieceArm",
            "type": "arm",
            "model": "ur",
            "host": "10.237.115.65"
        },
        {
            "name": "pieceGripper",
            "type": "gripper",
            "model": "robotiq",
            "host": "10.237.115.65",
            "frame" : {
                "parent": "world",
                "geometry": {
                  "x": 1,
                  "y": 2,
                  "z": 3,
                  "translation": {
                      "x": 4,
                      "y": 5,
                      "z": 6
                  }
                }
              }
        },
        {
            "name": "cameraOver",
            "type": "camera",
            "model": "single_stream",
            "attributes": {
                "host": "10.237.115.131",
                "port": 8181,
                "stream": "both"
            }
        },
        {
            "name": "wristCam",
            "api": "rdk:component:camera",
            "model": "rdk:builtin:url",
            "attributes": {
                "color": "http://10.237.115.65:4242/current.jpg"
            }
        }
