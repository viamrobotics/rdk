			"type": "gizmo",
			"attributes": {
				"arg1": "arg1"
			}
		},
		{
			"namespace": "rdk",
			"type": "base",
			"name": "shimmy",
			"model": "acme:demo:mybase",
			"depends_on": ["motor1", "motor2"],
			"attributes": {
				"motorL": "motor1",
				"motorR": "motor2"
			}
		}
	],
	"network": {
		"bind_address": ":8080"
