{
    "network": {
        "fqdn": "something-unique",
        "bind_address": ":8080"
    },
    "components":
    [
        {
            "model": "fake",
            "name": "arm1",
            "type": "arm",
            "service_config": [
                {
                  "type": "data_manager",
                  "attributes": {
                    "capture_methods": [
                        {
                            "method": "GetEndPosition",
                            "capture_frequency_hz": 100
                        }
                    ]
                  }
                }
            ]
        },
        {
            "model": "fake",
            "name": "cam1",
            "type": "camera",
            "service_config": [
                {
                  "type": "data_manager",
                  "attributes": {
                    "capture_methods": [
                        {
                            "method": "Next",
                            "capture_frequency_hz": 10,
                            "additional_params": {
                                "mime_type": "image/jpeg"
                            }
                        },
                        {
                            "method": "Next",
                            "capture_frequency_hz": 10,
                            "additional_params": {
                                "mime_type": "image/png"
                            }
                        }
                    ]
                  }
                }
             ]
            }
    ],
    "services":
    [
        {
            "type": "data_manager",
            "attributes": {
                "sync_interval_mins": 0,
                "capture_dir": "/tmp/capture"
            }
        }
    ]
}
