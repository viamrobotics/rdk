{
    "auth": {
        "handlers": [
            {
                "type": "api-key",
                "config": {
                    "key": "${RDK_AUTH_API_KEY}"
                }
            }
        ] 
    },
    "components":
    [
        {
            "model": "fake",
            "name": "arm1",
            "type": "arm"
        },
        {
            "model": "fake",
            "name": "base1_int",
            "type": "base"
        },
        {
            "model": "intercept_gps",
            "name": "base1",
            "type": "base",
            "attributes": {
                "base": "base1_int",
                "gps": "gps1",
                "start_latitude": 40.745297,
                "start_longitude": -74.010916
            },
            "depends_on": ["base1_int", "gps1"]
        },
        {
            "model": "fake",
            "name": "gripper1",
            "type": "gripper"
        },
        {
            "model": "fake",
            "name": "camera1",
            "type": "camera"
        },
        {
            "model": "fake",
            "name": "board1",
            "type": "board",
            "attributes":
            {
                "analogs":
                [
                    {
                        "name": "analog1",
                        "pin": "0"
                    }
                ],
                "digitalInterrupts":
                [
                    {
                        "name": "encoder",
                        "pin": "14"
                    }
                ]
            }
        },
        {
            "model": "fake",
            "name": "servo1",
            "type": "servo",
            "attributes":
            {
                "pin": "12"
            }
        },
        {
            "name": "g",
            "type": "motor",
            "model": "fake",
            "attributes":
            {
                "encoder": "encoder",
                "pins":
                {
                    "a": "1",
                    "b": "2",
                    "pwm": "3"
                },
                "ticksPerRotation": 100
            }
        },
        {
            "name": "gps1",
            "type": "gps",
            "model": "fake"
        }, 
        {
            "name": "imu1",
            "type": "imu",
            "model": "fake"
        },
        {
            "name": "input",
            "model": "fake",
            "type": "input_controller"
        },
        {
            "name": "forcematrix1",
            "type": "forcematrix",
            "model": "fake"
        }
    ],
    "processes":
    [
        {
            "id": "1",
            "name": "echo",
            "args":
            [
                "hello",
                "world"
            ],
            "one_shot": true
        },
        {
            "id": "2",
            "name": "bash",
            "args":
            [
                "-c",
                "trap \"exit 0\" SIGINT; while true; do echo hey; sleep 2; done"
            ],
            "log": true
        }
    ],
    "functions":
    [
        {
            "name": "func1",
            "engine": "javascript",
            "source": "(function() { console.log(robot.gripperOpen('gripper1')); return 1}())"
        },
        {
            "name": "func2",
            "engine": "javascript",
            "source": "(function() { console.log(robot.gripperGrab('gripper1')); return 2}())"
        }
    ],
    "services":
    [
        {
            "name": "navigation",
            "type": "navigation",
            "attributes": {
                "store": {
                    "type": "memory"
                },
                "gps": "gps1",
                "base": "base1"
            }
        }
    ]
}
