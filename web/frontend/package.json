{
  "name": "@viamrobotics/remote-control",
  "version": "2.27.0",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "copy-prime-assets": "node ./scripts/copy-prime-assets.js",
    "server": "ENV=development go run ./../cmd/server/main.go -debug -config ./../../etc/configs/fake.json",
    "start": "vite --host",
    "build": "vite build && npm run copy-prime-assets",
    "build-prod": "vite build --no-sourcemap && npm run copy-prime-assets",
    "build-types": "tsc --project tsconfig.build.json",
    "build-npm": "vite build --config vite.lib.config.ts && npm run build-types",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "preview": "vite preview",
    "_prettier": "prettier \"**/*.{ts,js,cjs,svelte,json,html,md}\"",
    "_eslint": "eslint \"**/*.{ts,js,svelte}\"",
    "lint": "npm run _prettier -- --check && npm run _eslint",
    "format": "npm run _prettier -- --write && npm run _eslint -- --fix",
    "cypress:ci": "cypress run",
    "cypress": "cypress open",
    "test:unit": "echo \"no tests exist yet\""
  },
  "files": [
    "dist"
  ],
  "module": "./dist/rc.js",
  "types": "./dist/main-lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main-lib.d.ts",
      "import": "./dist/rc.js"
    }
  },
  "peerDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@viamrobotics/prime": ">=0.5",
    "maplibre-gl": ">=4",
    "tailwindcss": ">=3.3",
    "three": ">=0.167"
  },
  "devDependencies": {
    "@mdi/js": "7.2.96",
    "@sveltejs/vite-plugin-svelte": "^2.4.4",
    "@threlte/core": "^7.3.1",
    "@threlte/extras": "^8.11.5",
    "@types/lodash-es": "^4.17.12",
    "@types/three": "0.167.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@viamrobotics/eslint-config": "^0.2.6",
    "@viamrobotics/prettier-config": "^0.3.4",
    "@viamrobotics/prime": "0.5.9",
    "@viamrobotics/prime-blocks": "^0.1.3",
    "@viamrobotics/prime-core": "^0.0.87",
    "@viamrobotics/sdk": "0.26.1",
    "@viamrobotics/three": "^0.0.3",
    "@viamrobotics/typescript-config": "^0.1.0",
    "cypress": "12.17.3",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-svelte": "^2.34.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unicorn": "48.0.1",
    "jshashes": "1.0.8",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^4.6.0",
    "postcss": "8.4.27",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "svelte": "^4.2.19",
    "svelte-check": "^3.5.2",
    "tailwindcss": "3.3.3",
    "three": "0.167.1",
    "three-inspect": "0.3.4",
    "trzy": "0.3.11",
    "type-fest": "^4.14.0",
    "typescript": "5.2.2",
    "vite": "4.5.5",
    "vite-plugin-css-injected-by-js": "3.3.0",
    "vitest": "0.34.1"
  }
}
