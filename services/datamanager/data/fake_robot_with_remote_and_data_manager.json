{
  "network": {
    "fqdn": "something-unique",
    "bind_address": ":8080"
  },
  "services": [
    {
      "name": "data_manager1",
      "type": "data_manager",
      "model": "builtin",
      "attributes": {
        "capture_dir": "/tmp/capture"
      }
    }
  ],
  "remotes": [
    {
      "name": "remote1",
      "address": "localhost:8081",
      "service_config": [
        {
          "name": "data_manager2",
          "type": "data_manager",
          "attributes": {
            "capture_methods": [
              {
                "name": "rdk:component:arm/remoteArm",
                "method": "EndPosition",
                "capture_frequency_hz": 100
              }
            ]
          }
        }
      ]
    }
  ]
}
